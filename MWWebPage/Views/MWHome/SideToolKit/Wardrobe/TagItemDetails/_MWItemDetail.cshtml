            <img src="" width="300" height="437">
            <img src="" width="300" height="437">

        </div>
        <div class="MW-item-upload-detaile" style="display:block;">
            <form action="">
                <input type="text" class="MW-item-tagName" placeholder="Tag" value="tag1 tag2 tag3">
                <div style="margin-top:2px">
                    <input type="text" class="MW-item-category" value="category" placeholder="category">
                    <input type="text" class="MW-item-color" value="color" placeholder="color">
                </div>
                <div style="margin-top:2px">
                    <input type="text" class="MW-item-brand" value="brand" placeholder="brand">
                    <input type="text" class="MW-item-price" value="price" placeholder="price">
                </div>
                <div>
                    <textarea class="MW-item-source" rows="1">@Global.source</textarea>
                </div>
                <textarea class="MW-item-descript" rows="2">@Global.discription</textarea>

            </form>
        </div>

        <div id="change-info">
            <button type="button" class="btn btn-primary btn-sm btn3d">@Global.change</button>
        </div>
    </div>
</div><!-- /.modal-dialog -->
</div><!-- /.modal -->
